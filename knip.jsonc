{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "ignore": ["generated/**/*.ts"],
    "entry": [
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "src/index.tsx!"
    ],
    "ignoreDependencies": [
        "@graphql-typed-document-node/core"
    ],
    "project": [
        "src/**/*.d.ts",
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "src/**/*.tsx!",
        "src/**/*.ts!",
        "generated/**/*.ts"
    ]
}
